do   
  for k in {10..20};   do     
    echo -e "K${k}s${seed}\t${k}\tMatrix_LDPrune_500_100_.15_S${seed}_K${k}.${k}.Q" >> Pong_mfile_SACE253.txt # Pong_mfile_k11-13_s1-10.txt   
  done; 
done
mv Pong_mfile_SACE253.txt Pong_mfile_SACE253_K10-k20.txt

